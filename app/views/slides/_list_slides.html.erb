<h1>Listing slides</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
<% @slides.each do |slide| %>
    <tr>
      <td><%= slide.title %></td>
      <td><%= slide.content %></td>
      <td><%= link_to 'Edit', "/presentations/#{slide.presentation.id}/slides/#{slide.id}/edit" %></td>
      <td><%= link_to 'Destroy',"/presentations/#{slide.presentation.id}/slides/#{slide.id}" , method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
    </tbody>
</table>

<br>


 